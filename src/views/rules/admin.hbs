{{#if rules}}
{{#each rules}}

<div class="col-md-10">
    <div class="card" >
        <div class="card-header">
            <h3>{{name}}</h3> 
        </div>
        <div class="card-body">
            <p>Name: {{name}} </p>
            <p>Description: {{description}} </p>
            <p>Format: {{format}} </p>
        </div> 
    </div>
</div>

{{/each}}
{{/if}}